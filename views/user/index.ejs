<h3>Users</h3>
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Email</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% users.forEach(function(user, index) { %>
			<tr data-id="<%= user.id %>" data-model="user">
				<td><%= user.id %></td>
				<td><%= user.username %></td>
				<td><%= user.email %></td>
				<td><a href="/user/show/<%= user.id %>">Show</a></td>
				<td><a href="/user/edit/<%= user.id %>">Edit</a></td>
				<form action="/user/destroy/<%= user.id %>" method="POST">
					<td>
						<input type="submit" value="Delete">
					</td>
				</form>
			</tr>
		<% })  %>
	</tbody>
</table>
<a href="/user/new" title="New">New</a>